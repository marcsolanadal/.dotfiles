snippet fn
  function(${1:args}) {
    ${2:body}
  }

snippet if
options head
  if(${1:condition}) {
     ${2:body}
  }

snippet cl
options head
  console.log(${1:message});

snippet cv
options head
  console.log('${1:variable}: ' + $1);

snippet cd
options head
  console.error('${1:variable} ------------------------------------------------);
  console.error('$1');
  console.error('--------------------------------------------------------------);
